@using Microsoft.AspNetCore.Http;
@using Microsoft.AspNetCore.Mvc;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>SNKRX - Admin</title>
    <!-- Bootstrap core CSS-->
    <link href="~/admintemplate2/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- Custom fonts for this template-->
    <link href="~/admintemplate2/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <!-- Page level plugin CSS-->
    <link href="~/admintemplate2/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">
    <!-- Custom styles for this template-->
    <link href="~/admintemplate2/css/sb-admin.css" rel="stylesheet">
    <script src="~/lib/moment-2.22.2/package/moment.js"></script>
</head>
<body id="page-top">

    @Html.Partial("_AdminTopMenuLayout.cshtml")


    <div id="wrapper">
        <!-- Sidebar -->
        @Html.Partial("_AdminLeftMenuLayout.cshtml")
        <div id="content-wrapper">
            <div class="container-fluid">

                @RenderBody()

            </div>
            <!-- /.container-fluid -->
            <!-- Sticky Footer -->
            <footer class="sticky-footer">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright © SneakersX 2018</span>
                    </div>
                </div>
            </footer>
        </div>
        <!-- /.content-wrapper -->
    </div>
    <!-- /#wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary" asp-area="Webmaster" asp-action="LogOut" asp-controller="Authentication">Logout</a>
                </div>
            </div>
        </div>
    </div>
    <!-- Bootstrap core JavaScript-->
    <script src="~/admintemplate2/vendor/jquery/jquery.min.js"></script>
    <script src="~/admintemplate2/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- Core plugin JavaScript-->
    <script src="~/admintemplate2/vendor/jquery-easing/jquery.easing.min.js"></script>
    <!-- Page level plugin JavaScript-->
    <script src="~/admintemplate2/vendor/chart.js/Chart.min.js"></script>
    <script src="~/admintemplate2/vendor/datatables/jquery.dataTables.js"></script>
    <script src="~/admintemplate2/vendor/datatables/dataTables.bootstrap4.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="~/admintemplate2/js/sb-admin.min.js"></script>
    <!-- chart -->
    <script src="~/admintemplate2/js/demo/chart-area-demo.js"></script>
    <script src="~/admintemplate2/js/demo/chart-bar-demo.js"></script>
    <script src="~/admintemplate2/js/demo/chart-pie-demo.js"></script>
    <script>
        $(document).ready(function()
        {
            LoadDuLieuArea($("#nbd").val(), $("#nkt").val());
            LoadDuLieuArea1($("#nbd").val(), $("#nkt").val());
            LoadDuLieuBar($("#nbd").val(), $("#nkt").val());
            //LoadDuLieuPie($("#nbd").val());
            LoadDuLieuPie();
        });
    </script>


</body>
</html>
