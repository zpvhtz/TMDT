@model IEnumerable<Models.Database.SizeSanPham>

<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th style="text-align: center; line-height: 74px;">Tên sản phẩm</th>
            <th style="text-align: center; line-height: 74px;">Size</th>
            <th style="text-align: center; line-height: 74px;">Số lượng</th>
        </tr>
    </thead>
    
    <tbody>
        @foreach (var item in Model)
        {
            if (item.SoLuong > 0)
            {
                <tr>
                    <td>@item.IdSanPhamNavigation.TenSanPham</td>
                    <td>@item.Size</td>
                    <td>@item.SoLuong</td>
                </tr>
            }
        }
    </tbody>
</table>