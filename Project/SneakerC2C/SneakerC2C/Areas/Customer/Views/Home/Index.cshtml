                        <div class="col-md-3 product-men">
                            <div class="men-pro-item simpleCart_shelfItem">
                                <div class="men-thumb-item">
                                    <a asp-area="Customer" asp-controller="SanPham" asp-action="Index" asp-route-id="@item.Id">
                                        <div class="anhsanpham-4" style="background-image: url(/Hinh/SanPham/@item.Hinh);"></div>
                                    </a>
                                </div>
                                <div class="item-info-product ">
                                    <h4>
                                        @if (item.TenSanPham.Length > 20)
                                        {
                                            <a asp-area="Customer" asp-controller="SanPham" asp-action="Index" asp-route-id="@item.Id">@item.TenSanPham.Substring(0, 18)...</a>
                                        }
                                        else
                                        {
                                            <a asp-area="Customer" asp-controller="SanPham" asp-action="Index" asp-route-id="@item.Id">@item.TenSanPham</a>
                                        }
