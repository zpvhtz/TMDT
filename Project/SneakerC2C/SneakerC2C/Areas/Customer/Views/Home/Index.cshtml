                                        @if (item.GiamGia != null && item.GiamGia != 0)
                                        {
                                            double gia = item.Gia * (100 - item.GiamGia) / 100;

                                            <span class="item_price">@gia.ToString("#,###") đ</span>
                                            <del>@item.Gia.ToString("#,###") đ</del>
                                        }
                                        else
                                        {
                                            <span class="item_price">@item.Gia.ToString("#,###") đ</span>
                                        }
