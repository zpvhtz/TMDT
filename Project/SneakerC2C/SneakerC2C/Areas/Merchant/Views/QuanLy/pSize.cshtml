@model IEnumerable<Models.Database.SizeSanPham>


<table table table-striped table-bordered table-hover>
    <thead>
        <tr>
            <th>Tên sản phẩm</th>
            <th>Size</th>
            <th>Số lượng</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.IdSanPhamNavigation.TenSanPham</td>
            <td>@item.Size</td>
            <td>@item.SoLuong</td>
        </tr>
        }
    </tbody>
</table>
